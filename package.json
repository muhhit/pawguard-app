{
  "name": "pawguard-mvp",
  "version": "1.0.0",
  "description": "PawGuard - AI-powered lost pet recovery platform",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "server": "cd server && npm run dev",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "spec": "node scripts/spec-kit.js",
    "spec:gemini": "node scripts/spec-kit.js -p google -t mobile:gemini",
    "spec:claude": "node scripts/spec-kit.js -p anthropic -t mobile:claude",
    "spec:openai": "node scripts/spec-kit.js -p openai -t mobile:openai",
    "spec:generate": "node scripts/spec-generate.js",
    "spec:analyze": "node scripts/spec-analyze.js",
    "spec:auto": "node scripts/spec-orchestrator.js --start T0",
    "spec:next": "node scripts/spec-orchestrator.js --start T0 --no-watch -n 1",
    "spec:dashboard": "node scripts/spec-dashboard.js",
    "test:watch": "jest --watch",
    "build": "expo export",
    "doctor": "npx expo-doctor"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-community/slider": "4.5.2",
    "@react-navigation/native": "^7.1.6",
    "@shopify/flash-list": "1.6.4",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@supabase/supabase-js": "^2.57.0",
    "@tanstack/react-query": "^5.83.0",
    "@ungap/structured-clone": "^1.3.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "expo": "^53.0.4",
    "expo-apple-authentication": "~6.4.2",
    "expo-auth-session": "~5.5.2",
    "expo-blur": "~13.0.3",
    "expo-camera": "~15.0.16",
    "expo-clipboard": "~6.0.3",
    "expo-constants": "~16.0.2",
    "expo-crypto": "~13.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.10",
    "expo-haptics": "~13.0.1",
    "expo-image": "~1.13.0",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-location": "~17.0.1",
    "expo-notifications": "~0.28.19",
    "expo-router": "~3.5.24",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-symbols": "~0.1.5",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "h3-js": "^4.3.0",
    "lottie-react-native": "6.7.0",
    "lucide-react-native": "^0.475.0",
    "nativewind": "^4.1.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-maps": "1.14.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-super-grid": "^6.0.2",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "react-native-webview": "13.8.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/eslintrc": "^3.1.0",
    "@expo/ngrok": "^4.1.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.31.0",
    "eslint-config-expo": "~7.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "typescript": "~5.3.3",
    "jest-expo": "^51.0.0"
  },
  "keywords": [
    "react-native",
    "expo",
    "pets",
    "lost-found"
  ],
  "author": "PawGuard Team",
  "license": "MIT",
  "private": true,
  "packageManager": "npm@10.8.1"
}