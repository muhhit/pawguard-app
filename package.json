{
  "name": "pawguard-mvp",
  "version": "1.0.0",
  "description": "PawGuard - AI-powered lost pet recovery platform",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "server": "cd server && npm run dev",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "spec": "node scripts/spec-kit.js",
    "spec:gemini": "node scripts/spec-kit.js -p google -t mobile:gemini",
    "spec:claude": "node scripts/spec-kit.js -p anthropic -t mobile:claude",
    "spec:openai": "node scripts/spec-kit.js -p openai -t mobile:openai",
    "spec:generate": "node scripts/spec-generate.js",
    "spec:analyze": "node scripts/spec-analyze.js",
    "spec:auto": "node scripts/spec-orchestrator.js --start T0",
    "spec:next": "node scripts/spec-orchestrator.js --start T0 --no-watch -n 1"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/native": "^7.1.6",
    "@shopify/flash-list": "1.7.6",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@supabase/supabase-js": "^2.57.0",
    "@tanstack/react-query": "^5.83.0",
    "@ungap/structured-clone": "^1.3.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "expo": "^53.0.4",
    "expo-apple-authentication": "~7.2.4",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.11",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.4",
    "expo-crypto": "~14.1.5",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.0",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.4",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.6",
    "expo-splash-screen": "~0.30.7",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "^14.2.0",
    "h3-js": "^4.3.0",
    "lottie-react-native": "7.2.2",
    "lucide-react-native": "^0.475.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "^0.79.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-super-grid": "^6.0.2",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.0",
    "@types/react": "~19.0.10",
    "eslint": "^9.31.0",
    "eslint-config-expo": "^9.2.0",
    "typescript": "~5.8.3",
    "concurrently": "^8.2.2"
  },
  "keywords": ["react-native", "expo", "pets", "lost-found"],
  "author": "PawGuard Team",
  "license": "MIT",
  "private": true
}
